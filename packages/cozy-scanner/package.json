{
  "name": "cozy-scanner",
  "version": "0.0.1",
  "description": "Cozy-Scanner is used to access device comera and also provide component to categorize document",
  "main": "dist/index.js",
  "license": "MIT",
  "homepage": "https://github.com/cozy/cozy-libs/blob/master/packages/cozy-scanner/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-libs.git"
  },
  "bugs": {
    "url": "https://github.com/cozy/cozy-libs/issues"
  },
  "scripts": {
    "build": "yarn run copy-files && env BABEL_ENV=transpilation yarn babel src/ --out-dir dist",
    "copy-files": "cp -rf src/locales dist/",
    "prepublishOnly": "yarn build",
    "test": "jest test/ --verbose",
    "lint": "cd .. && yarn lint packages/cozy-scanner",
    "tx": "tx pull --all",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "babel-preset-cozy-app": "^1.7.0",
    "jest": "24.1.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "stylus": "^0.54.7"
  },
  "peerDependencies": {
    "cozy-client": "7.4.1",
    "cozy-ui": "^25.7.0"
  },
  "sideEffects": false
}
