    "jest": "24.1.0",
    "preact": "^8.4.2",
    "preact-portal": "^1.1.3",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0"
  },
  "peerDependencies": {
    "cozy-client": "7.13.0",
    "cozy-device-helper": "1.7.1",
