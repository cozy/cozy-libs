{
  "name": "cozy-harvest-lib",
  "version": "1.14.4",
  "description": "Provides logic, modules and components for Cozy's harvest applications.",
  "main": "dist/index.js",
  "author": "Cozy",
  "license": "MIT",
  "homepage": "https://github.com/cozy/cozy-libs/blob/master/packages/cozy-harvest-lib/README.md",
