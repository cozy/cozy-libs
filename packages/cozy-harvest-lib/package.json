    "lodash": "4.17.15",
    "microee": "^0.0.6",
    "node-polyglot": "^2.4.0",
    "preact-portal": "^1.1.3",
    "react-final-form": "3.7.0",
    "react-markdown": "4.2.2",
    "react-router-dom": "^5.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "babel-jest": "24.1.0",
