    "react-markdown": "4.0.6",
    "react-proptypes": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "babel-jest": "24.1.0",
    "babel-preset-cozy-app": "^1.5.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.1.0",
