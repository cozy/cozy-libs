{
  "name": "renovate-config-cozy",
  "version": "0.7.0",
  "description": "Renovate config for Cozy (Cozy Cloud)",
  "renovate-config": {
    "default": {
      "extends": [
        "config:base"
      ],
      "rangeStrategy": "pin",
      "packageRules": [
        {
          "excludePackageNames": [
            "^cozy-"
          ],
          "extends": [
            "schedule:monthly"
          ]
        }
      ],
      "timezone": "Europe/Paris",
      "addLabels": ["dependencies"],
      "updateNotScheduled": false
    }
  },
  "license": "MIT"
}
