{
  "name": "twake-i18n",
  "version": "0.2.0",
  "description": "Twake-i18n provides everything to deal with translation",
  "main": "dist/index.js",
  "license": "MIT",
  "homepage": "https://github.com/cozy/cozy-libs/blob/master/packages/twake-i18n/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-libs.git"
  },
  "bugs": {
    "url": "https://github.com/cozy/cozy-libs/issues"
  },
  "scripts": {
    "build": "yarn build:clean && yarn build:types && env BABEL_ENV=transpilation babel --extensions .ts,.tsx,.js,.jsx,.md,.styl,.json,.snap ./src -d ./dist --copy-files --no-copy-ignored --verbose",
    "build:clean": "rm -rf ./dist",
    "build:types": "tsc -p tsconfig-build.json",
    "start": "yarn build --watch",
    "prepublishOnly": "yarn build",
    "test": "env NODE_ENV=test jest",
    "lint": "cd .. && yarn eslint --ext js,jsx packages/twake-i18n"
  },
  "devDependencies": {
    "@babel/cli": "7.16.8",
    "@babel/core": "7.16.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^14.4.0",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "babel-plugin-inline-json-import": "0.3.2",
    "babel-preset-cozy-app": "^2.8.2",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.1",
    "jest-environment-jsdom": "26.6.2",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-test-renderer": "16.12.0",
    "typescript": "5.5.2"
  },
  "dependencies": {
    "date-fns": "2.30.0",
    "lodash": "^4.17.21",
    "node-polyglot": "^2.4.0"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  }
}
