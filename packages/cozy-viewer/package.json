{
  "name": "cozy-viewer",
  "version": "23.3.2",
  "description": "Cozy-Viewer provides a component to show files in a viewer.",
  "main": "dist/index.js",
  "license": "MIT",
  "homepage": "https://github.com/cozy/cozy-libs/blob/master/packages/cozy-viewer/Readme.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-libs.git"
  },
  "bugs": {
    "url": "https://github.com/cozy/cozy-libs/issues"
  },
  "scripts": {
    "build": "yarn build:clean && yarn build:types && env BABEL_ENV=transpilation babel --extensions .ts,.tsx,.js,.jsx,.md,.styl,.json,.snap ./src -d ./dist --copy-files --no-copy-ignored --verbose",
    "build:clean": "rm -rf ./dist",
    "build:types": "tsc -p tsconfig-build.json",
    "start": "yarn build --watch",
    "prepublishOnly": "yarn build",
    "test": "env NODE_ENV=test jest",
    "lint": "cd .. && yarn eslint --ext js,jsx packages/cozy-viewer"
  },
  "devDependencies": {
    "@babel/cli": "7.16.8",
    "@babel/core": "7.16.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "^3.2.1",
    "babel-plugin-inline-json-import": "0.3.2",
    "babel-preset-cozy-app": "^2.8.1",
    "cozy-client": "^57.0.0",
    "cozy-device-helper": "2.0.0",
    "cozy-harvest-lib": "^33.0.0",
    "cozy-intent": "^2.30.0",
    "cozy-logger": "^1.17.0",
    "cozy-sharing": "^26.0.1",
    "cozy-ui": "^126.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.1",
    "jest-environment-jsdom": "26.6.2",
    "prop-types": "15.8.1",
    "react-dom": "16.12.0",
    "react-test-renderer": "16.12.0",
    "stylus": "0.63.0",
    "typescript": "5.5.2"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.21",
    "react-markdown": "^4.0.8",
    "react-pdf": "^5.7.2"
  },
  "peerDependencies": {
    "cozy-client": ">=57.0.0",
    "cozy-device-helper": ">=2.0.0",
    "cozy-harvest-lib": ">=32.2.12",
    "cozy-intent": ">=2.30.0",
    "cozy-logger": ">=1.17.0",
    "cozy-sharing": ">=25.4.0",
    "cozy-ui": ">=126.0.0",
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0"
  }
}
