{
  "name": "cozy-authentication",
  "version": "2.0.4",
  "description": "Component providing login to a Cozy",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "author": "Cozy",
  "license": "MIT",
  "scripts": {
    "prepare": "yarn build; yarn copy-files",
    "build": "env BABEL_ENV=transpilation yarn babel src/ --out-dir dist",
    "watch": "yarn build -w",
    "test": "env USE_REACT=true yarn jest src",
    "copy-files": "cp -r src/locales/ dist/locales/",
    "lint": "cd ..; yarn lint packages/cozy-authentication",
    "example": "nf start",
    "example-server": "env USE_REACT=true parcel --port 1234 $(pwd)/examples/index.html"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-preset-cozy-app": "^1.7.0",
    "cozy-client": "6.66.0",
    "cozy-ui": "22.14.1",
    "cssnano-preset-advanced": "^4.0.7",
    "date-fns": "^1.29.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint-cli": "1.1.1",
    "foreman": "^3.0.1",
    "jest": "24.5.0",
    "parcel": "^1.12.3",
    "postcss-merge-rules": "4.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "7.1.1",
    "react-router": "3",
    "react-styleguidist": "6.5.3",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "cozy-device-helper": "^1.8.0",
    "localforage": "1.7.3",
    "prop-types": "15.7.2",
    "snarkdown": "1.2.2",
    "url-polyfill": "1.1.7"
  },
  "jest": {
    "testURL": "http://localhost:8000",
    "transformIgnorePatterns": [
      "/node_modules/(?!cozy-ui)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ],
    "modulePaths": [
      "<rootDir>/node_modules/",
      "<rootDir>/../../node_modules/"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|styl)$": "identity-obj-proxy",
      "^cozy-client$": "cozy-client/dist/index.js"
    }
  },
  "peerDependencies": {
    "cozy-client": "6.66.0",
    "cozy-ui": "^22.4.0",
    "react": ">=15",
    "react-router": "3"
  }
}
