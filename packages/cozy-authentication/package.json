{
  "name": "cozy-authentication",
  "version": "2.3.3",
  "description": "Component providing login to a Cozy",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "author": "Cozy",
  "license": "MIT",
  "scripts": {
    "prepare": "yarn build; yarn copy-files",
    "build": "env BABEL_ENV=transpilation yarn babel src/ --out-dir dist",
    "watch": "yarn build -w",
    "test": "yarn jest src",
    "copy-files": "cp -r src/locales/ dist/locales/",
    "lint": "cd ..; yarn lint packages/cozy-authentication",
    "example": "nf start",
    "example-server": "parcel --port 1234 $(pwd)/examples/index.html"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "babel-preset-cozy-app": "^1.9.3",
    "cozy-client": "13.15.1",
    "cozy-ui": "35.39.0",
    "cssnano-preset-advanced": "^4.0.7",
    "date-fns": "^1.29.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint-cli": "1.1.1",
    "foreman": "^3.0.1",
    "jest": "26.2.2",
    "parcel": "^1.12.3",
    "postcss-merge-rules": "4.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "7.1.1",
    "react-router": "3",
  },
  "dependencies": {
    "cozy-device-helper": "^1.10.3",
    "localforage": "1.7.3",
    "prop-types": "15.7.2",
    "snarkdown": "1.2.2",
    "url-polyfill": "1.1.7"
  },
  "jest": {
    "testURL": "http://localhost:8000",
    "transformIgnorePatterns": [
      "/node_modules/(?!cozy-ui)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ],
    "modulePaths": [
      "<rootDir>/node_modules/",
      "<rootDir>/../../node_modules/"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|styl)$": "identity-obj-proxy",
      "^cozy-client$": "cozy-client/dist/index.js"
    }
  },
  "peerDependencies": {
    "cozy-client": "^13.15.1",
    "cozy-ui": "^35.39.0",
    "react": ">=15",
    "react-router": "3"
  }
}
