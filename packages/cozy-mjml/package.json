  },
  "devDependencies": {
	"@vercel/ncc": "0.34.0",
    "mjml": "4.13.0"
  },
  "scripts": {
	"build": "mkdir -p tmp dist && ncc build node_modules/.bin/mjml -o tmp/ncc && cp tmp/ncc/index.js dist/main.js && rm -rf tmp",
