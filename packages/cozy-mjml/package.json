  "license": "MIT",
  "homepage": "https://github.com/cozy/cozy-libs/blob/master/packages/cozy-mjml/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-libs.git"
  },
  "bugs": {
