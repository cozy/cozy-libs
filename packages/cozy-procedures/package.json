{
  "name": "cozy-procedures",
  "version": "0.18.1",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "yarn build; yarn copy-files",
    "watch": "env BABEL_ENV=transpilation yarn babel src/ --out-dir dist --watch",
    "build": "env BABEL_ENV=transpilation yarn babel src/ --out-dir dist && yarn run copy-files",
    "copy-files": "cp -rf src/locales dist/",
    "test": "jest --verbose",
    "lint": "cd ..; yarn lint packages/cozy-procedures"
  },
  "dependencies": {
    "babel-preset-cozy-app": "1.5.1",
    "lodash": "4.17.11",
    "react-jsonschema-form": "1.6.1",
    "react-redux": "7.1.0",
    "react-router": "3",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-starter-kit": "0.5.1"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@material-ui/core": "3.9.3",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "cozy-ui": "^21.0.0",
    "enzyme-to-json": "3.3.5",
    "jest": "24.8.0"
  },
  "peerDependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "cozy-ui": "^20.24.0"
  },
  "jest": {
    "setupFiles": [
      "./jestSetup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "styl"
    ],
    "moduleNameMapper": {
      "\\.(png|gif|jpe?g|svg)$": "<rootDir>/test/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/transpiled/",
      "/dist/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "<rootDir>/babel-transformer.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!cozy-ui)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
